<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    <style name="CheckBoxBlackBlock">
        <item name="android:textSize">10.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/pink_text</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:background">@drawable/selector_checkbox_black_block</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">30.0dip</item>
        <item name="android:button">@null</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="CheckBoxBlackBlockVertical">
        <item name="android:textSize">10.0sp</item>
        <item name="android:textStyle">bold</item>
